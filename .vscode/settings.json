{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": {"when": "$(basename).py"},
        "**/__pycache__": true,
        "**/.pytest_cache": true
    },
    "python.pythonPath": "env\\Scripts\\python.exe",
    "python.testing.pytestArgs": [
        "swagger_server"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.testing.pytestEnabled": true
}